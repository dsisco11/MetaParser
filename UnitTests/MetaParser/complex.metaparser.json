{
  "namespace": "UnitTestParser",
  "tokens": {
    "keyword_var": {
      "$type": "constant",
      "value": "var"
    },
    "keyword_function": {
      "$type": "constant",
      "value": "function"
    },
    "keyword_byte": {
      "$type": "constant",
      "value": "byte"
    },
    "keyword_short": {
      "$type": "constant",
      "value": "short"
    },
    "keyword_int": {
      "$type": "constant",
      "value": "int"
    },
    "keyword_float": {
      "$type": "constant",
      "value": "float"
    },
    "char_open_bracket": {
      "$type": "constant",
      "value": "{"
    },
    "char_close_bracket": {
      "$type": "constant",
      "value": "}"
    },
    "char_open_sqbracket": {
      "$type": "constant",
      "value": "["
    },
    "char_close_sqbracket": {
      "$type": "constant",
      "value": "]"
    },
    "char_open_parenthesis": {
      "$type": "constant",
      "value": "("
    },
    "char_close_parenthesis": {
      "$type": "constant",
      "value": ")"
    },
    "char_asterisk": {
      "$type": "constant",
      "value": "*"
    },
    "char_solidus": {
      "$type": "constant",
      "value": "/"
    },
    "char_reverse_solidus": {
      "$type": "constant",
      "value": "\\"
    },
    "whitespace": {
      "$type": "compound",
      "consume": [ " ", "\t", "\f" ]
    },
    "digits": {
      "$type": "compound",
      "consume": { "range": [ "0", "9" ] }
    },
    "letters": {
      "$type": "compound",
      "consume": [
        { "range": [ "a", "z" ] },
        { "range": [ "A", "Z" ] }
      ]
    },
    "newline": {
      "$type": "compound",
      "consume": "\n"
    },
    "identifier": {
      "$type": "complex",
      "start": [ "letters" ],
      "consume": [ "letters", "digits" ]
    },
    "comment": {
      "$type": "complex",
      "start": [ "char_solidus", "char_asterisk" ],
      "end": [ "char_asterisk", "char_solidus" ],
      "escape": [ "char_reverse_solidus" ]
    },
    "codeblock": {
      "$type": "complex",
      "start": [ "char_open_bracket" ],
      "end": [ "char_close_bracket" ]
    }
  }
}