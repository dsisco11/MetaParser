{
  "namespace": "Foo.Bar.Tokens",
  "tokens": {
    "keyword_var": {
      "$type": "constant",
      "value": "var"
    },
    "keyword_function": {
      "$type": "constant",
      "value": "function"
    },
    "left_curly_bracket": {
      "$type": "constant",
      "value": "{"
    },
    "right_curly_bracket": {
      "$type": "constant",
      "value": "}"
    },
    "whitespace": {
      "$type": "compound",
      "consume": [ " ", "\t", "\f" ]
    },
    "digits": {
      "$type": "compound",
      "consume": { "range": [ "0", "9" ] }
    },
    "letters": {
      "$type": "compound",
      "consume": [
        { "range": [ "a", "z" ] },
        { "range": [ "A", "Z" ] }
      ]
    },
    "newline": {
      "$type": "compound",
      "consume": "\n"
    },
    "identifier": {
      "$type": "complex",
      "start": [ "letters" ],
      "consume": [ "letters", "digits" ]
    }
  }
}